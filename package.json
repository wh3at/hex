{
	"name": "hex",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16",
		"bun": ">=1.0.0"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "bun run build && bun dist/cli.js",
		"start:dev": "bun source/cli.tsx",
		"test": "bun x biome check . && ava",
		"lint": "bun x biome lint .",
		"format": "bun x biome format . --write",
		"check": "bun x biome check --write ."
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@browserbasehq/stagehand": "^2.4.2",
		"dotenv": "^16.4.5",
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"playwright": "^1.54.2",
		"react": "^18.2.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"ink-testing-library": "^3.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3"
	},
	"ava": {}
}
